  <body>
    <%- include("../combonants/dark-light.ejs") %>

    


<main class="d-flex flex-nowrap">
  

  <%- include('../combonants/sidebar.ejs', {currentPage: "view"}) %>


  <section class="border w-100">
    <%- include("../combonants/navbar.ejs") %>


    <nav
    style="width: 88%" 
    class="px-3 mt-4 d-flex flex-wrap justify-content-between" aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page"><%= obj.fireName %> <%= obj.lastName %></li>
        </ol>

        <p class="">
          <samp class="fw-modium fs-6">Id :</samp> <%= obj._id %>
        </p>
      </nav>


      
    <table style="width: 88%;" class=" ms-1 table table-striped table-bordered mt-5">
        <thead>
          <tr>
            <th scope="col">First Name</th>
            <th class="fw-normal" scope="col"><%= obj.fireName %></th>
            
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">Last Name</th>
            <td><%= obj.lastName %></td>
          </tr>
          <tr>
            <th scope="row">Telephone</th>
            <td><%= obj.Telephone %></td>
          </tr>
          <tr>
            <th scope="row">Email</th>
            <td><%= obj.Email %></td>
          </tr>
          <tr>
            <th scope="row">Gender</th>
            <td><%= obj.Gender %></td>
          </tr>
          <tr>
            <th scope="row">Age</th>
            <td><%= obj.Age %></td>
          </tr>
          <tr>
            <th scope="row">Country</th>
            <td><%= obj.Country %></td>
          </tr>
          <tr>
            <th scope="row">Created on</th>
            <td><%= obj.createdAt %></td>
          </tr>
          <tr>
            <th scope="row">Last Update</th>
            <td><%= obj.updatedAt %></td>
          </tr>
        </tbody>
      </table>
  </section>
</main>

<script src="../js/bootstrap.bundle.min.js"></script>
    <script src="../js/sidebars.js"></script>
    <script src="../js/main.js" ></script>
  </body>
</html>
