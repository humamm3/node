  <body>
    <%- include("../combonants/dark-light.ejs") %>

    


<main class="d-flex flex-nowrap">
  

  <%- include('../combonants/sidebar.ejs', {currentPage:"edit"}) %>


  <section class="border w-100">
    <%- include("../combonants/navbar.ejs") %>
    


    <nav class="px-3 mt-4 d-flex flex-wrap justify-content-between" aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page">ali hasan</li>
        </ol>

        <p class="me-md-5"><samp class="fw-modium fs-6">Id :</samp> <%= obj._id %></p>
      </nav>


<form action="/edit/<%= obj._id %>?_method=PUT" method="post" class="row mx-0 gy-4 gx-3 mt-2">
  <div class="col-md-6">
    <label for="inputfrestName4" class="form-label">Frest Name</label>
    <input 
    value="<%= obj.fireName %>"
    name="fireName"
    type="text" 
    class="form-control" 
    id="inputfrestName4">
  </div>
  <div class="col-md-6">
    <label for="inputlastName4" class="form-label">Last Name</label>
    <input 
    value="<%= obj.lastName %>"
    name="lastName"
    type="text" class="form-control" id="inputlastName4">
  </div>


  <div class="col-md-6">
    <label for="inputemil4" class="form-label">Email:</label>
    <input 
    value="<%= obj.email %>"
    name="email" type="emil" class="form-control" id="inputemil4">
  </div>
  <div class="col-md-6">
    <label for="inputtele4" class="form-label">Telephone</label>
    <input value="<%= obj.phoneNumber %>"
    name="phoneNumber" type="number" class="form-control" id="inputtele4">
  </div>
  <div>
     <div class="col-md-6">
      <label for="inputage4" class="form-label">Age</label>
      <input value="<%= obj.age %>"
      name="age" type="number" class="form-control" id="inputage4">
     </div>
  </div>


  
 
  <div class="col-md-6">
    <label for="inputCountry" class="form-label">Country</label>
    <select name="Country" id="inputCountry" class="form-select">
